<% if @current_user %>
  

<div class="container">
  <div class="row">
    <div class="col-9 pt-4">
      <h2>Places</h2>
      <ul>
      <% for place in @places %>
        <li><a href="/places/<%= place.id %>"><%= place.name %></a></li>
      <% end %>
      </ul>
    </div>
    <div class="col-3 pt-4">
      <a href="/places/new/" class="btn btn-success">New Place</a>
    </div>
  </div>
</div>
  <% end %>